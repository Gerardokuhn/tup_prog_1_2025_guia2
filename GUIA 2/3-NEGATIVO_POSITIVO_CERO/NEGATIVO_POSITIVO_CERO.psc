Proceso NEGATIVOPOSITIVOCERO
	Definir NUM Como Real;
	Escribir 'INGRESE EL NUMERO A EVALUAR';
	Leer NUM;
	Si NUM<0 Entonces
		Escribir 'EL NUMERO INGRESADO ES NEGATIVO';
	FinSi
	Si NUM>0 Entonces
		Escribir 'EL NUMERO INGRESADO ES POSITIVO';
	FinSi
	Si NUM==0 Entonces
		Escribir 'EL NUMERO INGRESADO ES CERO';
	FinSi
FinProceso
